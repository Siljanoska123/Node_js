<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <p>With <%= name %></p>
    <ul> 
      <% names.forEach((name) => { %>
        <li><%= name %></li> 
      <%  })  %>
    </ul>
    <br>
    <p><b>List of Countries</b></p>
      <ul> 
        <% countries.forEach((country) => { %>
          <% if(country.ime === "Macedonia") { %>
          
          <li><%= country.ime %> Cela da e!!!</li>
          <% } else { %>
            <li> <%=country.ime %></li>
          <% } %> 
          
        <%  })  %>
      </ul>

      <br>


  </body>
</html>